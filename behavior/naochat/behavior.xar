                                                <script language="4">
                                                    <content>
                                                        <![CDATA[import subprocess, os, re, urllib2

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.tts = ALProxy("ALTextToSpeech")

    def onLoad(self):
        #~ puts code for box initialization here
        try:
            import poster
        except:
            import sys
            sys.path.append(ALFrameManager.getBehaviorPath(self.behaviorId))
            import poster
        self.poster = poster
    def onUnload(self):
        #~ puts code for box cleanup here
        pass

    def onInput_onStart(self):
        langage = self.tts.getLanguage()

        langMap = {'English':'en', 'French':'fr', 'German':'de', 'Italian':'it', 'Spanish':'es'} # FCA: we should handle this NAOqi should provide all this lang mapping with I18N
        try:
            lang = langMap[langage]
        except:
            # TODO handle error
            self.log("need to add a lang mapping for " + str(langage))
            lang = langMap['English']

        self.log("used language for Google STT = " + str(lang))
        try:
            #subprocess.check_call(["ffmpeg", "-y", "-i", "/home/nao/recordings/microphones/recording.wav", "-acodec", "pcm_s16le", "/home/nao/recordedSounds/out.wav"])
            filename2="/home/nao/brainlog/exelog"
            f2 = open(filename2, 'ab')
            f2.write("["+str(datetime.datetime.now())+"]: "+"[STT]Conversion a flac\n")
            subprocess.check_call(["flac", "/home/nao/recordedSounds/recording.wav", "-f", "--best", "--channel-map=none", "-V", "--sample-rate=44100", "-o", "/home/nao/recordedSounds/out.flac"])

            # Register the streaming http handlers with urllib2
            self.poster.streaminghttp.register_openers()
            # Encode audio file
            datagen, headers = self.poster.encode.multipart_encode({"audio": open("/home/nao/recordedSounds/out.flac", "rb")})
            headers['Content-Type']= 'audio/x-flac; rate=44100'
            # Create the Request object
            apikey="&key="
            #apikey+="AIzaSyBl30hoK07PA8sHYEcJYJ7iU5bKfHL0O6M" #1 THESIS
            apikey+="AIzaSyCP-v1eC9TzWmIxCIFDZcYXDvaYKaCcYPw" #2 API PROJECT
            #apikey+="AIzaSyBUPkUBgYQ2KvneXyoW8-cDVAfPuZ_ZO8U" #3 CHAT
            request = urllib2.Request("https://www.google.com/speech-api/v2/recognize?output=json&lang=" + str(lang)+apikey, datagen, headers)
            # Actually do the request, and get the response
            f2.write("["+str(datetime.datetime.now())+"]: "+"[STT]API Request\n")
            response = urllib2.urlopen(request, timeout=10).read()
            f2.write("["+str(datetime.datetime.now())+"]: "+"[STT]API Response\n")
            f2.close()
            filename="/home/nao/brainlog/apilog"
            f = open(filename, 'ab')
            f.write("--"+"["+str(datetime.datetime.now())+"]--\n"+str(response)+"--\n")
            f.close()
            # Parse response
            response = [e.split(':') for e in re.sub(r'[]{[}\n"]|(alternative":)', '', response).split(',')]
            text=response[0][3]
            conf=float(response[1][1])
            # Output response and log it
            filename3="/home/nao/brainlog/log"
            f3 = open(filename3, 'ab')
            f3.write("confidence: ("+str(conf)+")\n")
            self.Confidence(conf)
            if( text and text.strip() and conf != 0):
                self.Transcript(text)
                f3.write("["+str(datetime.datetime.now())+"](q): "+text+"\n")
            elif( conf < 0.6 ):
                self.Transcript("ERROR")
                f3.write("["+str(datetime.datetime.now())+"](q):ERROR\n")
            else:
                self.Transcript("SAY")
                f3.write("["+str(datetime.datetime.now())+"](q):SAY\n")
            f3.close()
        except:
            self.Transcript("SAY")
            filename3="/home/nao/brainlog/log"
            f3 = open(filename3, 'ab')
            f3.write("confidence: (-1)\n")
            f3.write("["+str(datetime.datetime.now())+"](q):ERROR\n")
            self.Confidence(-1)
            #self.Error()
            f3.close()

    def onInput_onStop(self):
        self.onUnload() #~ it is recommanded to call onUnload of this box in a onStop method, as the code written in onUnload is used to stop the box as well
        pass]]>
</content>
